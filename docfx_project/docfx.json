{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Arcade Idle Engine",
      "_appName": "Arcade Idle Engine",
      "_appFooter": "<span>Made by the <a href=\"https://www.arcadebridge.dev/\">Arcade Bridge</a>.",
      "pdf": true,
      "_enableSearch": true,
      "_disableContribution": true,
      "_disableTocFilter": true,
      "_disableNavbar": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}